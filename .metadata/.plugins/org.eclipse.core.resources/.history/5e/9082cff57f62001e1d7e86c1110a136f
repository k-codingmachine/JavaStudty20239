package ex06;

@FunctionalInterface
interface Dddd{
	int roll();
}

public class Lamda9_1 {
	public static void main(String[] args) {
		Dddd dd = 
			
			() -> (int)Math.random()*6; //  문장이 하나면 {}생략

			
	int result = dd.roll();
	System.out.println(result);
		
	
	Runnable r1 = ()-> run((int)Math.random()*6);
	}

	private static Object run(int i) {
		return null;
	}

}
